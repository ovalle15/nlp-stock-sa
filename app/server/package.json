{
    "name": "server",
    "packageManager": "yarn@4.1.0",
    "main": "index.ts",
    "scripts": {
        "install:clean": "rm -rf .yarn/ node_modules/ && yarn install",
        "start:prod": "node dist/index.js",
        "start:dev": "yarn build && nodemon index.ts",
        "build": "webpack",
        "lint": "eslint . --ext .ts",
        "lint-and-fix": "yarn lint --fix",
        "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "nodemon": "^3.0.3"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/eslint": "^8",
        "@types/express": "^4.17.21",
        "@types/node": "^20.11.17",
        "@typescript-eslint/eslint-plugin": "^7.0.1",
        "@typescript-eslint/parser": "^5.0.1",
        "@webpack-cli/generators": "^3.0.7",
        "copy-webpack-plugin": "^12.0.2",
        "eslint": "^8.52.0",
        "prettier": "^3.1.0",
        "prettier-eslint": "^16.1.2",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "typescript": "^4.4.4",
        "webpack": "^5.90.1",
        "webpack-cli": "^5.1.4",
        "webpack-node-externals": "^3.0.0"
    }
}
