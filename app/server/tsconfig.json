{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Node 18",
    "_version": "18.2.0",
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "lib": ["es2023"],
        "module": "node16",
        "moduleResolution": "node16",
        "noImplicitAny": true,
        "outDir": "./dist",
        "rootDir": "./src",
        "target": "es2022",
        "allowJs": true,
        "strict": true
    },
    "files": ["src/index.ts"]
}
